<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config/>

    <bean id="pers" class="tema2.ex1.example19.Persoana">
        <property name="nume" value="dana"/>
    </bean>
    <bean id="job1" class="tema2.ex1.example19.Job">
        <property name="firma" value="BestCompany"/>
        <property name="functia" value="sef"/>
    </bean>
    <bean id="job2" class="tema2.ex1.example19.Job">
        <property name="firma" value="TheOtherBestCompany"/>
        <property name="functia" value="sef"/>
    </bean>
</beans>